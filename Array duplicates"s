
class Solution {
  public:
    vector<int> findDuplicates(vector<int>& arr) {
        // code here
        sort(arr.begin(),arr.end());
        vector<int>a;
        for(int i=0;i<arr.size()-1;i++)
        {
            if(arr[i]==arr[i+1])
            {
                a.push_back(arr[i]);
            }
        }
        return a;
    }
};
